{
  "name": "mkebus-graphql",
  "version": "1.0.0",
  "description": "A graphql server for MCTS's real time bus data",
  "main": "./dist/app.js",
  "scripts": {
    "test": "mocha  \"test/*.test.js\" --compilers js:babel-register",
    "test-watch": "mocha \"test/*.test.js\" -w --compilers js:babel-register",
    "start-watch": "nodemon lib/app.js --exec babel-node --presets es2015,stage-2",
    "start": "babel-node lib/app.js --presets es2015,stage-2",
    "lint": "eslint \"lib/**\" \"test/**\" "
  },
  "keywords": [
    "bus",
    "realtime",
    "milwaukee",
    "mke",
    "MCTS",
    "real-time"
  ],
  "author": "Travis Shears",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.3.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "moment": "^2.22.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.3",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
  }
}
